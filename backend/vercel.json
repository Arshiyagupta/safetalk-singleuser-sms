{
  "version": 2,
  "functions": {
    "dist/app.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/app.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "/dist/app.js"
    },
    {
      "src": "/health",
      "dest": "/dist/app.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/src/public/$1"
    },
    {
      "src": "/",
      "dest": "/dist/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/app.js"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  }
}